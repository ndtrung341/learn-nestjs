{
   "name": "learn-nestjs",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "env-cmd typeorm-ts-node-commonjs -d src/database/data-source.ts",
      "migration:run": "npm run typeorm migration:run",
      "migration:revert": "npm run typeorm migration:revert",
      "migration:show": "npm run typeorm migration:show",
      "migration:fresh": "npm run typeorm schema:drop && npm run migration:run",
      "make:migration": "npm run typeorm migration:generate src/database/migrations/%npm_config_name% -- -p",
      "make:seeder": "env-cmd typeorm-extension seed:create -n src/database/seeds/%npm_config_name%",
      "db:seed": "env-cmd typeorm-extension seed:run"
   },
   "dependencies": {
      "@nestjs-modules/mailer": "^2.0.2",
      "@nestjs/cache-manager": "^2.3.0",
      "@nestjs/common": "^10.4.15",
      "@nestjs/config": "^4.0.0",
      "@nestjs/core": "^10.4.15",
      "@nestjs/jwt": "^11.0.0",
      "@nestjs/mapped-types": "*",
      "@nestjs/passport": "^11.0.5",
      "@nestjs/platform-express": "^10.4.15",
      "@nestjs/schedule": "^5.0.1",
      "@nestjs/typeorm": "^11.0.0",
      "bcryptjs": "^3.0.1",
      "cache-manager": "^5.7.6",
      "cache-manager-ioredis-yet": "^2.1.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "cookie-parser": "^1.4.7",
      "dayjs": "^1.11.13",
      "file-type": "^20.4.1",
      "handlebars": "^4.7.8",
      "ms": "^2.1.3",
      "nodemailer": "^6.10.0",
      "passport": "^0.7.0",
      "passport-google-oauth20": "^2.0.0",
      "passport-jwt": "^4.0.1",
      "passport-local": "^1.0.0",
      "passport-microsoft": "^2.1.0",
      "pg": "^8.13.3",
      "reflect-metadata": "^0.2.2",
      "rxjs": "^7.8.1",
      "typeorm": "^0.3.21",
      "typeorm-extension": "^3.7.0",
      "uuid": "^11.1.0"
   },
   "devDependencies": {
      "@eslint/eslintrc": "^3.2.0",
      "@eslint/js": "^9.18.0",
      "@nestjs/cli": "^11.0.0",
      "@nestjs/schematics": "^11.0.0",
      "@nestjs/testing": "^10.4.15",
      "@swc/cli": "^0.6.0",
      "@swc/core": "^1.10.7",
      "@types/busboy": "^1.5.4",
      "@types/express": "^5.0.0",
      "@types/jest": "^29.5.14",
      "@types/ms": "^2.1.0",
      "@types/multer": "^1.4.12",
      "@types/node": "^22.10.7",
      "@types/nodemailer": "^6.4.17",
      "@types/passport-google-oauth20": "^2.0.16",
      "@types/passport-jwt": "^4.0.1",
      "@types/passport-local": "^1.0.38",
      "@types/passport-microsoft": "^1.0.3",
      "@types/supertest": "^6.0.2",
      "env-cmd": "^10.1.0",
      "eslint": "^9.18.0",
      "eslint-config-prettier": "^10.0.1",
      "eslint-plugin-prettier": "^5.2.2",
      "globals": "^15.14.0",
      "jest": "^29.7.0",
      "prettier": "^3.4.2",
      "source-map-support": "^0.5.21",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.5",
      "ts-loader": "^9.5.2",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.7.3",
      "typescript-eslint": "^8.20.0"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
